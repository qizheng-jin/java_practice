package day5;

import java.util.Arrays;

public class TwoD_array {
	
	public static void main(String[] args) {
		int[][] a = new int[10][10];
		for(int i=0; i<a.length; i++) {
			for(int j=0; j<a[i].length; j++) {
				a[i][j] = i+j;
			}
		}
		
		for(int i=0; i<a.length; i++) {
			for(int j=0; j<a[i].length; j++) {
				System.out.print(a[i][j] + "  ");
			}
		}
	}
}
