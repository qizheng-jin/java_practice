
public class First_java {
	/**
	 * 
	 * ц╟ещеепР
	 */
	public void bubbleSort(int[] a) {
		for(int i=0; i<a.length-1; i++) {
			if(a[i]>a[i+1]) {
				int temp = a[i];
				a[i] = a[i+1];
				a[i+1] = temp;
			}
		}
		System.out.println(a.toString());
	}
	
	
	public static void main(String[] args) {
		First_java fj = new First_java();
		fj.bubbleSort({3,2,1,4,5});
		
	}
}
