
public class First_java {
	/**
	 * 
	 * ц╟ещеепР
	 */
	public void bubbleSort(int[] a) {
		for(int i=0; i<a.length-1; i++) {
			if(a[i]>a[i+1]) {
				int temp = a[i];
				a[i] = a[i+1];
				a[i+1] = temp;
			}
		}
		for(int i:a) {
			System.out.println(i);
		}
	}
	
	
	public static void main(String[] args) {
		First_java fj = new First_java();
		int[] a = {1,2,3,6,5};
		fj.bubbleSort(a);
		
	}
}
