package cn.tedu.hello;

import java.util.Scanner;

public class For_loop {
	public static void main(String[] args) {
		int result = 8;
		int times = 10;
		for(int i=0; i<4; i++) {
			System.out.println(result);
			result = 10*result + 8;
		}
	//质数练习
	Scanner in = new Scanner(System.in);
	int a = in.nextInt();
	String s = "是质数";
	for(int i=2; i<a; i++) {
		if(a%i == 0) { 
			s = "不是质数";
			break;
			}
		}
	System.out.println(s);
	in.close();
	}
}
